menu "ISA-dependent Options for x86"

if PERF_OPT
choice
  prompt "Optimization for condition code evaulation"
  default x86_CC_SKIP
config x86_CC_SKIP
  bool "Skip CC"
config x86_CC_LAZY
  bool "Lazy CC"
config x86_CC_NONE
  bool "none"
endchoice
endif

if !PERF_OPT
config x86_CC_NONE
endif

config PA
  depends on MODE_SYSTEM
  bool "Enable PA version"
  default n

endmenu
